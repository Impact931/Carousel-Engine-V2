{
  "version": 2,
  "builds": [
    {
      "src": "carousel_engine/api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "carousel_engine/api/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "carousel_engine/api/main.py": {
      "maxDuration": 300
    }
  }
}